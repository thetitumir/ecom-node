<%- include("../common/header.ejs") -%>
    <div class="layout-page">
        <!-- Content wrapper -->
        <div class="content-wrapper">
            <!-- Content -->

            <div class="container-xxl flex-grow-1 container-p-y">

                <div class="row">
                    <% if(success && success ){ %>
                        <div class="alert alert-success alert-dismissible fade show">
                            <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
                            <h4 class="alert-heading">Success</h4>
                            <p>
                                <%= success %>
                            </p>
                        </div>
                        <% } %>
                            <form action="/admin/category/add" method="post" enctype="multipart/form-data"
                                class="form-control">
                                <lebel for="title">Category Title</lebel>
                                <input type="text" name="title" class="form-control" placeholder="Product Title"
                                    id="title">
                                <lebel for="category">Slug</lebel>
                                <input type="text" name="slug" class="form-control" placeholder="Slug">

                                <input type="submit" value="submit" class="btn btn-primary" name="submit">

                            </form>
                            <div>
                            </div>
                </div>
            </div>
            <%- include("../common/footer.ejs") -%>